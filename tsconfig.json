{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "strictNullChecks": false,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "rootDir": ".",
    "allowJs": true
  },
  "include": ["**/*"],
  "exclude": ["node_modules", "dist"],
  "lib": ["es2015"]
}
